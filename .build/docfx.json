{
  "metadata": [
    {
      "src": [
        {
	 "files": [ "StreamExtended.Docs.sln"],
         "src": "../src/"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
         "files": [ "*.md" ]
      }
    ],
    "resource": [
      {
        "files": [ ""]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "Stream Extended",
      "_enableSearch": true
    },
    "dest": "../docs",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
